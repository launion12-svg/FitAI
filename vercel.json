{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/api/gemini",
      "dest": "/api/gemini.js"
    }
  ],
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/ai-override.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}